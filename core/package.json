{
  "name": "react-1ogin-page",
  "version": "1.4.7",
  "description": "Some `react` login pages, which can be used quickly after installation.",
  "homepage": "https://github.com/Lola-blip-boop/react-login-page",
  "author": "lola palooza <lola.palooza.me@gmail.com>",
  "license": "MIT",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./cjs/index.d.ts"
    },
    "./logo": {
      "import": "./esm/logo/Logo.js",
      "require": "./cjs/logo/Logo.js"
    },
    "./logo-rect": {
      "import": "./esm/logo/LogoRect.js",
      "require": "./cjs/logo/LogoRect.js"
    },
    "./README.md": "./README.md",
    "./form-data.md": "./docs/form-data.md",
    "./create-login-page.md": "./docs/create-login-page.md",
    "./package.json": "./package.json"
  },
  "scripts": {
    "watch": "tsbb watch src/*.tsx --use-babel",
    "build": "tsbb build src/*.tsx --use-babel",
    "coverage": "tsbb test --env=jsdom --coverage --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lola-blip-boop/react-login-page.git"
  },
  "files": [
    "docs/*",
    "README.md",
    "*.d.ts",
    "src",
    "esm",
    "cjs"
  ],
  "peerDependencies": {
    "@babel/runtime": ">=7.11.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "keywords": [
    "react",
    "login",
    "login-page",
    "login-pages",
    "react-login",
    "react-login-page",
    "editor",
    "syntax",
    "ide",
    "code"
  ],
  "jest": {
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  },
  "types": "./cjs/index.d.ts",
  "bugs": {
    "url": "https://github.com/Lola-blip-boop/react-login-page/issues"
  },
  "directories": {
    "doc": "docs"
  }
}
